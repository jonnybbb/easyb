<ivysettings>
  <conf defaultResolver="chained"/>
  <resolvers>
    <chain name="chained" returnFirst="true">
      <url name="discorepo">
        <artifact pattern="http://www.thediscoblog.com/repo/[artifact]/jars/[artifact]-[revision].[ext]"/>
      </url>
      <ibiblio name="ibiblio"/>
    </chain>
  </resolvers>
</ivysettings>